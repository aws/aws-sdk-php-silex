{
    "name": "aws/aws-sdk-php-silex",
    "homepage": "http://aws.amazon.com/sdkforphp2",
    "description": "A simple Silex service provider for including the AWS SDK for PHP.",
    "keywords": ["silex","aws","amazon","sdk","s3","ec2","dynamodb"],
    "type":"library",
    "license":"Apache-2.0",
    "authors":[
        {
            "name":"Amazon Web Services",
            "homepage":"http://aws.amazon.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "aws/aws-sdk-php": "~3.0",
        "silex/silex": "1.*",
        "aws/aws-sdk-php-v3-bridge": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Aws\\Silex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aws\\Silex\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3"
    }
}
